{
  "name": "aios-nexus",
  "version": "5.0.0",
  "description": "AIOS Nexus - Framework Unificado de Desenvolvimento com IA - PREVC Workflow, 15 Agents, MCP Server, Multi-IDE Sync",
  "bin": {
    "aios": "bin/aios.js",
    "aios-core": "bin/aios.js",
    "aios-nexus": "bin/aios-nexus.js",
    "aios-context": "bin/aios-context.js",
    "aios-tui": "bin/aios-tui.js"
  },
  "preferGlobal": false,
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "bin/",
    "src/",
    "scripts/",
    "packages/",
    ".aios-core/",
    ".context/",
    ".claude/",
    "tests/fixtures/",
    "README.md",
    "README-NEXUS.md",
    "CHANGELOG-NEXUS.md",
    "LICENSE"
  ],
  "main": "src/index.js",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "format": "prettier --write \"**/*.md\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "typecheck": "tsc --noEmit",
    "release": "semantic-release",
    "sync:ide": "node bin/aios-nexus.js sync",
    "validate": "node bin/aios-nexus.js validate",
    "mcp": "node bin/aios-context.js",
    "prepublishOnly": "npm run generate:manifest"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@kayvan/markdown-tree-parser": "^1.5.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ansi-to-html": "^0.7.2",
    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "glob": "^10.4.4",
    "handlebars": "^4.7.8",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "inquirer": "^8.2.7",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "picocolors": "^1.1.1",
    "proper-lockfile": "^4.1.2",
    "react": "^18.3.1",
    "semver": "^7.7.2",
    "validator": "^13.15.15",
    "yaml": "^2.7.1",
    "zod": "^4.3.6"
  },
  "keywords": [
    "ai",
    "aios",
    "aios-nexus",
    "agile",
    "agents",
    "mcp",
    "model-context-protocol",
    "orchestrator",
    "fullstack",
    "development",
    "cli",
    "cross-platform",
    "interactive",
    "workflow",
    "prevc",
    "multi-ide",
    "scaffolding"
  ],
  "author": "Guilherme Giorgi <contato@ggailabs.com>",
  "contributors": [
    "Genesis Grid AI Labs <contato@ggailabs.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggailabs/aios-nexus.git"
  },
  "bugs": {
    "url": "https://github.com/ggailabs/aios-nexus/issues"
  },
  "homepage": "https://ggailabs.com/aios-nexus",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.1.1",
    "mocha": "^11.7.5",
    "prettier": "^3.5.3",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "yaml-lint": "^1.7.0"
  }
}
