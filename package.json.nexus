{
  "name": "aios-nexus",
  "version": "5.0.0-alpha.1",
  "description": "AIOS Nexus - Framework Unificado de Desenvolvimento com IA. Multi-IDE Sync, AI Scaffolding, MCP Server, Quality Gates.",
  "main": "src/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aios-nexus": "./bin/aios-nexus.js",
    "aios-context": "./bin/aios-context.js",
    "aios-scaffold": "./bin/aios-scaffold.js"
  },
  "files": [
    "bin/",
    "src/",
    "dist/",
    ".context/",
    ".aios-core/",
    "README.md",
    "LICENSE"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node bin/aios-nexus.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "sync": "node src/cli/commands/sync.js",
    "sync:ide": "node src/cli/commands/sync.js",
    "sync:validate": "node src/cli/commands/sync.js --validate",
    "context:init": "node src/cli/commands/context.js init",
    "context:fill": "node src/cli/commands/context.js fill",
    "workflow:start": "node src/cli/commands/workflow.js start",
    "mcp:serve": "node bin/aios-context.js",
    "release": "semantic-release",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "aios",
    "nexus",
    "agents",
    "scaffolding",
    "mcp",
    "cli",
    "multi-ide",
    "context-engineering",
    "workflow",
    "quality-gates",
    "prevc"
  ],
  "author": "GG.AI Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ggailabs/aios-nexus.git"
  },
  "bugs": {
    "url": "https://github.com/ggailabs/aios-nexus/issues"
  },
  "homepage": "https://ggailabs.com/aios-nexus",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.9",
    "@ai-sdk/google": "^3.0.6",
    "@ai-sdk/openai": "^3.0.7",
    "@clack/prompts": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "ai": "^6.0.19",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ansi-to-html": "^0.7.2",
    "axios": "^1.6.0",
    "boxen": "^7.1.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "execa": "^5.1.1",
    "fast-glob": "^3.3.3",
    "figures": "^3.2.0",
    "fs-extra": "^11.3.2",
    "glob": "^10.4.4",
    "handlebars": "^4.7.8",
    "ink": "^4.4.0",
    "inquirer": "^12.6.3",
    "js-yaml": "^4.1.0",
    "marked": "^9.1.0",
    "node-cron": "^3.0.0",
    "ora": "^7.0.0",
    "picocolors": "^1.1.1",
    "proper-lockfile": "^4.1.2",
    "react": "^18.2.0",
    "semver": "^7.7.2",
    "validator": "^13.15.15",
    "ws": "^8.14.0",
    "yaml": "^2.3.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/cli-progress": "^3.11.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.1.1",
    "prettier": "^3.5.3",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "yaml-lint": "^1.7.0"
  },
  "optionalDependencies": {
    "tree-sitter": "^0.21.0",
    "tree-sitter-typescript": "^0.23.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
